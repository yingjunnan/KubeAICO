{
  "name": "kubeaico-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean:generated": "find src -type f -name '*.js' -delete && find src -type f -name '*.js.map' -delete && rm -f tsconfig.tsbuildinfo",
    "typecheck": "vue-tsc --noEmit",
    "dev": "npm run clean:generated && vite",
    "build": "npm run clean:generated && npm run typecheck && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "echarts": "^5.5.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vue-tsc": "^2.0.24"
  }
}
